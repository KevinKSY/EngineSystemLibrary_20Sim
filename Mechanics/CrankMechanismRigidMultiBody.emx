   IC 472 456
    description '<Information>
 <Description>
    <Version>4.2</Version>
<IsMainModel>1</IsMainModel>
  <KeepParameterValues>False</KeepParameterValues>
    <LibraryPath>Bond Graph\IC.emx</LibraryPath>
  <TimeStamp>2011-11-29 16:01:19</TimeStamp>
</Description>
</Information>';
    type IC
     ports
      power in p1;
      power out p2;
      signal out theta;
     restrictions
      causality fixed in p1;
      causality fixed out p2;
    end;
    icon bg bottom
     figures
      text 'IC' 472 456 18 bold;
    end;
    implementation eq
parameters
	real global S {m};  						//Stroke
	real global l_CR {m};					//Length of the connecting rod
	real global jCR1, jCR2 {kg.m2/rad};			//mass monent of inertia of conneting rod and crank rod respectively
	real global mP, mCR1, mCR2 {kg}; //mass of piston, conneting rod and crank rod respectively
variables
	real p_theta;			//momentum of the system
	real r;			//Length of the crank rod
	real I;			//General inertia of the system
	real hidden m1;
initialequations
	r = S / 2;				
equations
	p_theta = int (p1.e);
	theta = int (p2.f);
	m1 = (r^2*cos(theta)*sin(theta))/ (2*l_CR*sqrt(1 - (r^2*sin(theta)^2)/l_CR^2));
	I = jCR2 + mCR1*(r*sin(theta) + m1)^2 + mP*(r*sin(theta)   + m1/2)^2 
     - (jCR1*r^2*cos(theta)^2)/(l_CR^2*((r^2*sin(theta)^2)/l_CR^2 - 1));
	p1.f = p_theta / I;
	p2.e = -g_n*(mCR1*(r*sin(theta) + m1) + mP*(r*sin(theta) + m1/2) + (mCR2*r*sin(theta))/2);


implementation_end;
   MTF 352 376
    description '<Information>
 <Description>
    <Version>4.2</Version>
<IsMainModel>1</IsMainModel>
  <KeepParameterValues>False</KeepParameterValues>
    <LibraryPath>Bond Graph\MTF.emx</LibraryPath>
  <TimeStamp>2011-11-29 16:15:31</TimeStamp>
</Description>
</Information>';
    type MTF
     ports
      power in p1;
      power out p2;
      signal in theta;
     restrictions
      causality constraint not_equal p1 p2;
    end;
    icon bg bottom
     figures
      text 'MTF' 352 376 18 bold;
    end;
    implementation eq
// Transformation of translation movement to rotational movement in crank mechanism
parameters
	real global l_CR;	//length of connecting rod
	real global S;		  //stroke
variables
	real r_CR;				//length of crank rod
	real lambda; 			//ratio of length of crank rod and connecting rod
	real m;						//modulus of transformation
initialequations
	r_CR = 0.5*S;	
	lambda = r_CR / l_CR;
equations
	m = -r_CR*(sin(theta) + 2*lambda*sin(2*theta)/sqrt(1-(lambda*sin(theta))^2));
	p2.e	= m * p1.e;
	p1.f = m * p2.f;
implementation_end;
