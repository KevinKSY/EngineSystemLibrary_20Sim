      power in p1;
      power out p2;
     restrictions
      causality constraint not_equal p1 p2;
    end;
    icon bg bottom
     figures
      text 'TF' 352 304 18 bold;
    end;
    implementation eq
parameters
	real global B;  // Bore of the piston
variables
	real areaPiston;
initialequations
	areaPiston = B^2/4*	pi;	
equations
	p2.e = areaPiston * p1.e;
	p1.f	= areaPiston * p2.f;
implementation_end;
